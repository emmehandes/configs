"pamixer --decrease 2"
    m:0x90 + c:48
    Mod2+Mod5 + minus
"pamixer --increase 2"
    m:0x90 + c:20
    Mod2+Mod5 + plus
"pamixer --toggle-mute"
    m:0x90 + c:49
    Mod2+Mod5 + section

"mpc toggle"
    m:0x80 + c:35
    Mod5 + dead_diaeresis
"mpc next"
    m:0x80 + c:114
    Mod5 + Right
"mpc prev"
    m:0x80 + c:113
    Mod5 + Left


