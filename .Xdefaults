! urxvt
urxvt.xftAntialias:      true
Antialias: 1
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.rgba: rgb

Xft*antialias:          true
Xft*hinting:            full

/* urL-yanking */
urxvt.keysym.M-u: perl:mark-yank-urls:activate_mark_mode
urxvt.underlineurLs: true
urxvt.perl-lib: /home/xintron/.urxvt/
urxvt.perl-ext: selection,mark-yank-urls
urxvt.urlLauncher: firefox --display=:0.1

urxvt.termName:    rxvt-256color
urxvt.urgentOnBell: true
urxvt.cursorBlink: false
urxvt.transparent: false 
urxvt.shading:          100
urxvt.scrollBar:        false
urxvt.saveLines:        4096
urxvt.mouseWheelScrollPage: true
urxvt.perl-ext-common: default,matcher
urxvt.matcher.button: 2
urxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-] 


/* Colors */
urxvt.background: #121212
urxvt.foreground: #bbbbbb
urxvt.cursorColor: #A7A15E
urxvt.cursorColor2: #252525
urxvt.colorIT: #FFFFFF
urxvt.colorBD: #FFFFFF
urxvt.colorUL: #FF8F00
urxvt.colorRV: #333333
urxvt.underlineColor: #0E0E0E
urxvt.borderColor: #0E0E0E
urxvt.pointerColor: #252525
urxvt.pointerColor2: #252525

/* fonts */
!urxvt.font: -*-profont-*-*-*-*-11-*-*-*-*-*-*-*
!urxvt.boldFont: -*-profont-*-*-*-*-11-*-*-*-*-*-*-*
urxvt.font: -*-montecarlo-medium-*-*-*-11-110-72-72-c-60-*-*
urxvt.boldFont: -*-montecarlo-bold-*-*-*-11-110-72-72-c-60-*-*
urxvt.italicFont: -*-profont-medium-*-*-*-11-*-*-*-*-*-*-*

!black
urxvt.color0: #000000
urxvt.color8: #444444

!red
urxvt.color1: #F25C05
urxvt.color9: #F00000

!green
urxvt.color2: #3E5916
urxvt.color10: #93A605

!brown/yellow
urxvt.color3: #F28C05
urxvt.color11: #FFE863

!blue
urxvt.color4: #185ED8
urxvt.color12: #6899EE

!magenta
urxvt.color5: #E80C7A
urxvt.color13: #FF0DFF

!cyan
urxvt.color6: #6C98BF
urxvt.color14: #A0C3D9

!white
urxvt.color7: #DDDDDD
urxvt.color15: #E5EFFA
